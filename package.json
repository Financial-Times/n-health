{
  "name": "n-health",
  "version": "0.0.0",
  "description": "Makes it easy to add healthchecks to an app",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "dotcom-tool-kit test:local",
    "prepare": "npx snyk protect || npx snyk protect -d || true",
    "build": "dotcom-tool-kit build:local",
    "start": "dotcom-tool-kit run:local"
  },
  "engines": {
    "node": "16.x || 18.x",
    "npm": "7.x || 8.x || 9.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Financial-Times/n-health.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Financial-Times/n-health/issues"
  },
  "homepage": "https://github.com/Financial-Times/n-health#readme",
  "devDependencies": {
    "@dotcom-tool-kit/component": "^4.1.0",
    "@dotcom-tool-kit/husky-npm": "^4.2.0",
    "@dotcom-tool-kit/lint-staged-npm": "^3.2.0",
    "@dotcom-tool-kit/mocha": "^3.2.0",
    "chai": "^4.3.6",
    "dotcom-tool-kit": "^3.4.0",
    "lintspaces-cli": "^0.8.0",
    "mocha": "^10.0.0",
    "proxyquire": "^1.7.4",
    "sinon": "^14.0.0",
    "snyk": "^1.167.2"
  },
  "dependencies": {
    "@dotcom-reliability-kit/logger": "^2.2.6",
    "aws-sdk": "^2.6.10",
    "fetchres": "^1.5.1",
    "moment": "^2.29.4",
    "ms": "^2.0.0",
    "node-fetch": "^2.6.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dotcom-tool-kit git:precommit",
      "commit-msg": "dotcom-tool-kit git:commitmsg"
    }
  },
  "lint-staged": {
    "**/*.js": "dotcom-tool-kit format:staged test:staged --"
  },
  "volta": {
    "node": "18.16.0"
  }
}
